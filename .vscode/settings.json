    "npm.enableScriptExplorer": true,
    "jestrunner.configPath": "jest-config-vscode.js",
    "javascript.implicitProjectConfig.experimentalDecorators": true,
    "eslint.alwaysShowStatus": true,
    "eslint.run": "onSave"
}